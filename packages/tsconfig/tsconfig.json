{
  "compilerOptions": {
    "outDir": "./lib",
    "lib": [
      "es2020"
    ],
    "declaration": true,
    "sourceMap": true,
    "target": "es2020",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
