{
  "name": "@kiwi/gpt-orchestrators",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev:ts": "nodemon --exec ts-node src/index.ts",
    "lab": "node src/__labs__.js",
    "start": "node dist/index.js"
  },
  "nodemonConfig": {
    "delay": 1000,
    "env": {
      "NODE_ENV": "development"
    },
    "exec": "ts-node --transpile-only src/index.ts",
    "ext": "ts,js,json",
    "ignore": [
      "src/**/*.spec.ts",
      "src/**/*.test.ts",
      "node_modules/**/*",
      "dist/**/*"
    ],
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@kiwi/common": "*",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "openai": "^4.91.0",
    "serve-static": "^2.2.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@kiwi/tsconfig": "*",
    "@types/node": "^22.15.24",
    "nodemon": "^2.0.22",
    "typescript": "^5.8.3"
  }
}
